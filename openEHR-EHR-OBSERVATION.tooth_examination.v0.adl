			purpose = <"Record of the examination of a single tooth, primarily intended for use within the Mouth Examination archetype">
			use = <"Used as a subunit of the Mouth Examination archetype">
			keywords = <"tooth, teeth", "dental", "odonto", "dentistry", "orthodontic", "palmer", "FDI", "notation", "mouth", "caries">
