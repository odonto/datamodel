definition
	OBSERVATION[at0000] matches {	-- Tooth examination
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {	-- Tooth Type
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0038, 	-- Deciduous Incisor
													at0039, 	-- Deciduous Canine
													at0040, 	-- Deciduous Molar
													at0041, 	-- Deciduous Supernumerary
													at0042, 	-- Permanent Incisor
													at0043, 	-- Permanent Canine
													at0044, 	-- Permanent Molar
													at0045, 	-- Permanent Premolar
													at0046]	-- Permanent Supernumerary
												}
											}
										}
									}
									ELEMENT[at0047] occurrences matches {0..1} matches {	-- Tooth Status
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0048, 	-- Present
													at0049, 	-- Absent
													at0050]	-- Replaced
												}
											}
										}
									}
									CLUSTER[at0007] occurrences matches {0..1} matches {	-- Canonical Location
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0006] occurrences matches {0..1} matches {	-- Upper/Lower
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0010, 	-- Upper
															at0011]	-- Lower
														}
													}
												}
											}
											ELEMENT[at0008] occurrences matches {0..1} matches {	-- Left/Right
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0012, 	-- Left
															at0013]	-- Right
														}
													}
												}
											}
											ELEMENT[at0014] occurrences matches {0..1} matches {	-- Numerical Index
												value matches {
													DV_COUNT matches {
														magnitude matches {|1..10|}
													}
												}
											}
										}
									}
									ELEMENT[at0020] occurrences matches {0..1} matches {	-- Exposure
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0021, 	-- Erupted
													at0022, 	-- Unerupted
													at0023]	-- Partially Erupted
												}
											}
										}
									}
									CLUSTER[at0024] occurrences matches {0..1} matches {	-- Orientation
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0025] occurrences matches {0..1} matches {	-- Rotation
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0035, 	-- Mesially
															at0036]	-- Distally
														}
													}
												}
											}
											ELEMENT[at0026] occurrences matches {0..1} matches {	-- Inclination
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0030, 	-- Proclined
															at0031, 	-- Retroclined
															at0032, 	-- Buccal
															at0033, 	-- Lingual
															at0034]	-- Palatal
														}
													}
												}
											}
										}
									}
									allow_archetype ELEMENT[at0037] occurrences matches {0..*} matches {	-- Tooth Pathology
										include
											archetype_id/value matches {/.*/}
									}
								}
							}
						}
					}
				}
			}
		}
	}

